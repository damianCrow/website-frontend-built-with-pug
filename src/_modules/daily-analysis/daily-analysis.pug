include ../square/square

mixin daily-analysis(baseUrl)
  .daily-analysis
    +square({modifier: [], meta: {text: 'Daily Analysis', date: false}, title: {text: "Sample text"}})
      a.square__bottom-link(href='https://twitter.com') View Analysis
    +square({modifier: ['bright'], meta: {text: 'Daily Analysis', date: false}, title: {text: "Sample text"}})
      a.square__bottom-link(href='https://twitter.com') 2 hours ago
    a.daily-analysis__bottom-link(href='') View All Technical Analysis

